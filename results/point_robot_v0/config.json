{
  "algo": "PPO",
  "env_name": "PointRobot-v0",
  "env_configs": {
    "episode_length": 100
  },
  "use_linear_lr_decay": false,
  "actor_lr": 0.0004,
  "critic_lr": 0.0004,
  "optimizer_eps": 1e-05,
  "max_grad_norm": 0.5,
  "random_seed": 6785,
  "cuda_deterministic": false,
  "use_cuda": true,
  "policy_iterations": 100,
  "meta_episodes_per_epoch": 250,
  "meta_episode_length": 1000,
  "num_processes": 20,
  "discount_gamma": 0.99,
  "use_proper_time_limits": true,
  "ppo_opt_epochs": 10,
  "ppo_clip_param": 0.1,
  "ppo_entropy_coef": 0.01,
  "ppo_value_loss_coef": 0.5,
  "ppo_num_minibatches": 50,
  "use_gae": true,
  "gae_lambda": 0.95,
  "directory": "./results/point_robot_v0/",
  "log_interval": 1,
  "checkpoint_interval": 1,
  "eval_interval": 1
}