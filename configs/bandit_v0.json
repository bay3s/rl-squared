{
  "algo": "PPO",
  "env_name": "Bandit-v0",
  "env_configs": {
    "num_actions": 5
  },
  "actor_lr": 0.0001,
  "actor_wd": 0.003,
  "critic_lr": 0.0001,
  "critic_wd": 0.003,
  "use_linear_lr_decay": false,
  "optimizer_eps": 0.00001,
  "max_grad_norm": 0.5,
  "random_seed": 4536,
  "cuda_deterministic": false,
  "use_cuda": true,
  "total_steps": 5000000,
  "meta_episode_length": 100,
  "meta_episodes_per_epoch": 100,
  "num_processes": 10,
  "discount_gamma": 0.99,
  "use_proper_time_limits": false,
  "ppo_opt_epochs": 10,
  "ppo_num_minibatches": 20,
  "ppo_clip_param": 0.1,
  "ppo_entropy_coef": 0.01,
  "ppo_value_loss_coef": 0.5,
  "use_gae": true,
  "gae_lambda": 0.3,
  "directory": "./results/bandit_v0/",
  "log_interval": 1,
  "checkpoint_interval": 1,
  "eval_interval": 1
}
